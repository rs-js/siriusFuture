{
  "name": "siriusfuture",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reset": "yarn start --reset-cache",
    "ios: device": "react-native run-ios --device 'iPhone'",
    "ios: se": "react-native run-ios --simulator 'iPhone SE (1st generation)'",
    "ios: pod": "npx pod-install ios",
    "ios: xcode": "open ./ios/siriusFuture.xcworkspace",
    "ios: update": "npx pod-install ios --repo-update",
    "ios: clean": "cd ios && xcodebuild clean",
    "android: aab": "cd android && ./gradlew bundleRelease",
    "android: apk": "cd android && ./gradlew assembleRelease",
    "android: clean": "cd android && ./gradlew clean",
    "android: device": "react-native run-android --variant=release",
    "android: sign": "cd android && ./gradlew signingReport"
  },
  "dependencies": {
    "@react-native-seoul/masonry-list": "^1.0.8",
    "@react-navigation/bottom-tabs": "^6.0.8",
    "@react-navigation/native": "^6.0.5",
    "@react-navigation/native-stack": "^6.2.3",
    "axios": "^0.22.0",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/react-native": "^0.65.0",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-redux": "^7.1.19",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "babel-jest": "^27.2.5",
    "eslint": "^8.0.0",
    "jest": "^27.2.5",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-flipper": "^0.114.1",
    "react-test-renderer": "17.0.2",
    "redux-flipper": "^2.0.1",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
